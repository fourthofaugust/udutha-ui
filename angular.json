{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dev": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/dev",
      "sourceRoot": "projects/dev/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dev",
            "index": "projects/dev/src/index.html",
            "main": "projects/dev/src/main.ts",
            "polyfills": "projects/dev/src/polyfills.ts",
            "tsConfig": "projects/dev/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/dev/src/favicon.ico",
              "projects/dev/src/assets"
            ],
            "styles": [
              "projects/dev/src/styles.scss",
              "projects/foa/udutha-ui/src/lib/styles/udutha.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/dev/src/environments/environment.ts",
                  "with": "projects/dev/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dev:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dev:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dev:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dev/src/test.ts",
            "polyfills": "projects/dev/src/polyfills.ts",
            "tsConfig": "projects/dev/tsconfig.spec.json",
            "karmaConfig": "projects/dev/karma.conf.js",
            "assets": [
              "projects/dev/src/favicon.ico",
              "projects/dev/src/assets"
            ],
            "styles": [
              "projects/dev/src/styles.scss",
              "projects/foa/udutha-ui/src/lib/styles/udutha.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dev/tsconfig.app.json",
              "projects/dev/tsconfig.spec.json",
              "projects/dev/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/dev/e2e/protractor.conf.js",
            "devServerTarget": "dev:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dev:serve:production"
            }
          }
        }
      }
    },
    "foa-udutha-ui": {
      "projectType": "library",
      "root": "projects/foa/udutha-ui",
      "sourceRoot": "projects/foa/udutha-ui/src",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        }
      },
      "prefix": "uui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/foa/udutha-ui/tsconfig.lib.json",
            "project": "projects/foa/udutha-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/foa/udutha-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/foa/udutha-ui/src/test.ts",
            "tsConfig": "projects/foa/udutha-ui/tsconfig.spec.json",
            "karmaConfig": "projects/foa/udutha-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/foa/udutha-ui/tsconfig.lib.json",
              "projects/foa/udutha-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "foa-udutha-ui"
}
