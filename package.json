{
  "name": "@fourthofaugust/udutha-ui",
  "version": "1.0.7",
  "description": "a set of angular components built using vanilla-framework css by canonical",
  "repository": {
    "type": "git",
    "url": "https://github.com/fourthofaugust/udutha-ui.git"
  },
  "bugs": {
    "url": "https://github.com/fourthofaugust/udutha-ui/issues"
  },
  "homepage": "https://github.com/fourthofaugust/udutha-ui",
  "contributors": [
    "Phani Mahesh",
    "Aditya Peesapati"
  ],
  "maintainers": [
    "Phani Mahesh",
    "Aditya Peesapati"
  ],
  "keywords": [
    "angular components",
    "vanilla framework",
    "ux",
    "fourthofaugust",
    "udutha",
    "udutha ui"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:sandbox": "npm run build:prod-lib && ng serve dev",
    "build:prod-lib": "ng build foa-udutha-ui --prod",
    "build:prod-sandbox": "ng build dev --prod",
    "lint:lib": "ng lint foa-udutha-ui",
    "lint:sandbox": "ng lint dev",
    "lint:fix-lib": "ng lint foa-udutha-ui --fix",
    "lint:fix-sandbox": "ng lint dev --fix",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "vanilla-framework": "^2.12.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:lib && npm run lint:sandbox"
    }
  }
}
